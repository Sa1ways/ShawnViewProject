<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="text" format="string|reference"/>
    <attr name="textColor" format="color|reference"/>
    <attr name="textSize" format="dimension|reference"/>
    <attr name="backgroundColor" format="color|reference"/>

    <declare-styleable name="SortTextView">
        <attr name="text" />
        <attr name="textSize"/>
        <attr name="textDefaultColor" format="color|reference"/>
        <attr name="textSortColor" format="color|reference"/>
        <attr name="arrowSortColor" format="color|reference"/>
        <attr name="arrowDefaultColor" format="color|reference"/>
        <attr name="paddingLeft" format="dimension|reference"/>
        <attr name="paddingRight" format="dimension|reference"/>
        <attr name="paddingTop" format="dimension|reference"/>
        <attr name="paddingBottom" format="dimension|reference"/>
        <attr name="arrowPaddingLeftRight" format="dimension|reference"/>
        <attr name="arrowWidth" format="dimension|reference"/>
    </declare-styleable>

    <declare-styleable name="SubmitButton">
        <attr name="text"/>
        <attr name="textColor"/>
        <attr name="textSize"/>
        <attr name="backgroundColor"/>
        <attr name="circleDashColor" format="color|reference"/>
        <attr name="circleLightColor" format="color|reference"/>
        <attr name="tipsColor" format="color|reference"/>
        <attr name="collapseTime" format="integer|reference"/>
    </declare-styleable>

    <declare-styleable name="ProgressLoadView">
        <attr name="textSize" />
        <attr name="textColor"/>
        <attr name="backgroundColor"/>
        <attr name="progressBarHeight" format="dimension|reference"/>
        <attr name="indicatorPadding" format="dimension|reference"/>
        <attr name="progressBarMargin" format="dimension|reference"/>
    </declare-styleable>
</resources>